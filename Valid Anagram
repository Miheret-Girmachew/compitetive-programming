class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        anagram1={}
        anagram2={}
        for char in s:
            if char in anagram1:
                anagram1[char]+=1
            else:
                anagram1[char]=1
        for char2 in t:
            if char2 in anagram2:
                anagram2[char2]+=1  
            else:
                anagram2[char2]=1
        if anagram1 == anagram2:
            return True
        else:
            return False
            
                
            
        
        
        
        
        
        
        
        
        
        
       
            
